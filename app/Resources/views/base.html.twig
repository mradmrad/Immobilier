<html lang="fr"> <!--<![endif]-->

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="initial-scale=1.0,maximum-scale=1.0,user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Remove Tap Highlight on Windows Phone IE -->
    <meta name="msapplication-tap-highlight" content="no"/>

    <title>{% block title %}Green Duck immobilier{% endblock %}</title>

    {% block css %}
        {% stylesheets output="css/compiled/mainBase.css" filter="cssrewrite"
        'bundles/immobilier/bower_components/uikit/css/uikit.almost-flat.min.css'
        'bundles/immobilier/assets/css/login_page.min.css'
        'bundles/immobilier/assets/css/ie.css'
        'bundles/immobilier/assets/css/themes/_theme_g.css'
        %}
        <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}"/>
        {% endstylesheets %}
        <link rel="icon" type="image/x-icon" href="{{ asset('icon.ico') }}"/>
    {% endblock %}
</head>

<body class="login_page app_theme_g">
<div >

    {% block body %} {% endblock %}

    {#<footer id="footer">#}
        {#Â©#}
        {#<script> document.write(new Date().getFullYear()); </script>#}
        {#- <a target="_blank" href="http://www.sbc.tn">Smart Business Challenge</a>#}
    {#</footer>#}
</div>

{% block js %}

    {% javascripts output="js/compiled/mainBase.js"

    'bundles/immobilier/assets/js/common.min.js'
    'bundles/immobilier/assets/js/uikit_custom.min.js'
    'bundles/immobilier/assets/js/altair_admin_common.min.js'
    'bundles/immobilier/assets/js/pages/login.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}


{% endblock %}
</body>
</html>