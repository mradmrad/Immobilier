{% extends "::layout.html.twig" %}
{% block css %}
    {{ parent() }}
    {% stylesheets output="css/compiled/mainWithDropifyAndKedouiComobox.css" filter="cssrewrite"
    'bundles/immobilier/assets/skins/dropify/css/dropify.css'
    'bundles/immobilier/bower_components/kendo-ui/styles/kendo.common-material.min.css'
    'bundles/immobilier/bower_components/kendo-ui/styles/kendo.material.min.css' %}
    <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}
{% block title %}
    Modifier une acquisition  - {{ parent() }}
{% endblock %}
{% block body %}
    <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">
        <ul id="breadcrumbs" style="float: right;">
            <li><a href="#">Acquisitions</a></li>
            <li><span>Modifier une acquisition</span></li>
        </ul>
        <h1>Formulaire de modification de l'acquisition</h1>
        <span class="uk-text-muted uk-text-upper uk-text-small">veuillez bien remplir les champs</span>
    </div>
    {{ include("BienBundle:Acquisition:formNew.html.twig") }}
{% endblock %}
{% block js %}
    {{ parent() }}
    <script>
        altair_forms.parsley_validation_config();
    </script>
    {% javascripts output="js/compiled/mainWithDropifyMaskAndWysiwygAndFormatAndSFCollectionAnalyzerAndKendoui.js"
    'bundles/immobilier/assets/js/custom/dropify/dist/js/dropify.min.js'
    'bundles/immobilier/assets/js/kendoui_custom.min.js'
    'bundles/immobilier/assets/js/pages/forms_file_input.min.js'
    'bundles/immobilier/bower_components/parsleyjs/dist/parsley.min.js'
    'bundles/immobilier/assets/js/pages/forms_validation.min.js'
    'bundles/immobilier/jQuery-Mask-Plugin/js/jquery.mask.min.js'
    'bundles/immobilier/jquery-numberformatter/jshashtable-2.1.js'
    'bundles/immobilier/jquery-numberformatter/jquery.numberformatter-1.2.4.js'
    'bundles/immobilier/sfCollectionAnalyzer/sf_collection_analyzer.js'
    'bundles/immobilier/assets/js/kendoui_custom.js'
    'bundles/immobilier/assets/js/pages/kendoui.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}