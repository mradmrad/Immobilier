        function ajouterTache($typeTache, $mandat) {
            var $description = CKEDITOR.instances['descriptionn'].getData();
            var url = '{{ path('tache_mandat_new') }}';
            var data = {
                'typetache': $typeTache,
                'mandat': $mandat,
                'description': $description
            };
            $.ajax({
                async: true,
                type: "POST",
                url: url,
                dataType: 'json',
                data: data,
                success: function (json) {
                    if (json.success === false || json.message !== '') {
                        UIkit.modal.alert('erreur');
                    } else {
                        UIkit.modal.alert('La tâche a été ajoutée avec succès.', {
                                labels: {
                                    Ok: 'Fermer'
                                }
                            }
                        ).on('hide.uk.modal', function () {
                            location.reload(true);
                        });
                    }

                },
                error: function () {
                    UIkit.modal.alert('exception contacter SBC', {
                        labels: {
                            Ok: 'Fermer'
                        }
                    });
                }
            });
        }
        function newOffre() {
            var code = $('#codeOffre');
            var codeIsValid = code.parsley().isValid();
            if (codeIsValid) {
                var url = '{{ path('offre_add') }}';
                $.ajax({
                    async: true,
                    type: "POST",
                    url: url,
                    data: "code=" + code.val() + '&proposedBy=' + $('#proposedBy').val()
                    + '&proposedPrice=' + $('#proposedPrice').val() + '&proposedPriceText=' + $('#proposedPriceText').val()
                    + '&mandat=' + '{{ mandat.id }}',
                    success: function (json) {
                        if (json.success === false || json.message !== '') {
                            window.Parsley.addValidator('codeoffre', {
                                validateString: function () {
                                    return false;
                                },
                                messages: {
                                    en: 'Ce code existe déjà.',
                                    fr: "Ce code existe déjà"
                                }
                            });
                            code.on('change', function () {
                                window.Parsley.addValidator('code', {
                                    validateString: function () {
                                        return true;
                                    }
                                });
                                code.parsley().validate();
                            });
                        } else {
                            window.Parsley.addValidator('code', {
                                validateString: function () {
                                    return true;
                                }
                            });
                            UIkit.modal.alert('L\'offre a été ajoutée avec succès', {
                                labels: {
                                    Ok: 'Fermer'
                                }
                            }).on('hide.uk.modal', function () {
                                location.reload(true);
                            });
                        }
                        code.parsley().validate();
                    },
                    error: function () {
                        code.parsley().validate();
                    }
                });
            }
            else {
                code.parsley().validate();
            }
        }
