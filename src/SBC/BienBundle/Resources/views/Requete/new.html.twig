{% extends "::layout.html.twig" %}
{% block css %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('/bundles/immobilier/assets/skins/dropify/css/dropify.css') }}">
    {% stylesheets output="css/compiled/mainWithDropify.css" filter="cssrewrite"
    'bundles/immobilier/assets/skins/dropify/css/dropify.css' %}
    <link rel="stylesheet" type="text/css" media="screen" href="{{ asset_url }}"/>
    {% endstylesheets %}
{% endblock %}
{% block title %}
    Ajouter une requête - {{ parent() }}
{% endblock %}
{% block body %}
    <div id="page_heading" data-uk-sticky="{ top: 48, media: 960 }">
        <ul id="breadcrumbs" style="float: right;">
            <li><a href="#">Requêtes</a></li>
            <li><span>Ajouter une requête ({{ requete.reference }})</span></li>
        </ul>
        <h1>Formulaire d'ajout de la requête</h1>
        <span class="uk-text-muted uk-text-upper uk-text-small">veuillez bien remplir les champs</span>
    </div>
    {{ include("BienBundle:Requete:form.html.twig") }}
{% endblock %}
{% block js %}

    {{ parent() }}
    <script>
        altair_forms.parsley_validation_config();
    </script>
    {% javascripts output="js/compiled/mainWithMaskAndWysiwyg.js"
    'bundles/immobilier/assets/js/kendoui_custom.min.js'
    'bundles/immobilier/bower_components/parsleyjs/dist/parsley.min.js'
    'bundles/immobilier/assets/js/pages/forms_validation.min.js'
    'bundles/immobilier/jQuery-Mask-Plugin/js/jquery.mask.min.js'
    'bundles/immobilier/jquery-numberformatter/jshashtable-2.1.js'
    'bundles/immobilier/jquery-numberformatter/jquery.numberformatter-1.2.4.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script src="{{ asset('bundles/immobilier/bower_components/ion.rangeslider/js/ion.rangeSlider.js') }}"></script>
    <script src="{{ asset('bundles/immobilier/assets/js/uikit_htmleditor_custom.min.js') }}"></script>
    <!-- inputmask-->
    <script src="{{ asset('bundles/immobilier/bower_components/jquery.inputmask/dist/jquery.inputmask.bundle.js') }}"></script>

    <!--  forms advanced functions -->
    <script src="{{ asset('bundles/immobilier/assets/js/pages/forms_advanced.min.js') }}"></script>
    {#<script src="{{ asset('bundles/immobilier/assets/js/uikit_custom.min.js')}}"></script>#}
    {#<!-- altair common functions/helpers -->#}
    {#<script src="{{ asset('bundles/immobilier/assets/js/altair_admin_common.min.js')}}"></script>#}
    <script src="{{ asset('js/compiled/mainWithDropify_dropify.min_1.js') }}"></script>

    <!--  form file input functions -->
    {#<script src="{{ asset('template/assets/js/kendoui_custom.min.js') }}"></script>#}
    {#<script src="{{ asset('template/assets/js/kendoui_custom.min.js') }}"></script>#}
    <script src="{{ asset('bundles/immobilier/assets/js/pages/forms_file_input.min.js') }}"></script>

{% endblock %}