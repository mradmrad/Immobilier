{% block js %}
    {{ parent() }}
    {% javascripts output="js/compiled/peitystats.js"
    'bundles/immobilier/bower_components/peity/jquery.peity.min.js' %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script src="{{ asset('bundles/immobilier/assets/js/date_format.js') }}"></script>
    <script src="{{ asset('bundles/immobilier/assets/js/pages/forms_advanced.min.js') }}"></script>
    <script src="{{ asset('bundles/immobilier/bower_components/ion.rangeslider/js/ion.rangeSlider.min.js') }}"></script>
    <script>
        function deletePersonnel() {
            var url = '{{ path('personnel_delete', {'id': 'iD'}) }}';
            url = url.replace('iD', {{ personnel.id }});
            $.ajax({
                url: url,
                type: 'delete',
                success: function (json) {
                    if (json.success) {
                        UIkit.modal.alert('Le personnel a été supprimé avec succès.'
                        ).on('hide.uk.modal', function () {
                            history.back();
                        });
                    }
                    else {
                        UIkit.modal.alert('Vous n\'avez pas le droit de supprimer cet personnel pour des raisons de sécurité');
                    }
                },
                error: function (exception) {
                    console.log(exception.responseText);
                    UIkit.modal.alert('Vous n\'avez pas le droit de supprimer ce personnel pour des raisons de sécurité');
                }
            });
        }
        document.onreadystatechange = function () {
            if (document.readyState === "complete") {
                $(".peity_orders").peity("donut", {
                    height: 40,
                    width: 40,
                    fill: ["#8bc34a", "#eee"]
                });
                $(".peity_orders_red").peity("donut", {
                    height: 40,
                    width: 40,
                    fill: ["#a91101", "#eee"]
                });
                $(".peity_orders_orange").peity("donut", {
                    height: 40,
                    width: 40,
                    fill: ["#ffa500", "#eee"]
                });
            }
        }

        function date_filter() {
            altair_helpers.content_preloader_show('md');
            var longDateFormat = 'dd/MM/yyyy HH:mm:ss';
            var begin = $('#uk_dp_start').val();
            var end = $('#uk_dp_end').val();
            if (begin == '')
                begin = null;
            if (end == '')
                end = null;


            var url = '{{ path('personnel_filter_taches', {'id': 'iD' , 'begin' : 'BEGIN' , 'end' : 'END'}) }}';
            url1 = url.replace('iD', {{ personnel.id }});
            url2 = url1.replace('BEGIN', begin);
            url = url2.replace('END', end),
                $.ajax({
                    url: url,

                    success: function (json) {
                        $('#taches').html('');
                        if (json.liste.length > 0) {
                            jQuery(json.liste).each(function (i, item) {

                                if (item.bien == null) {
                                    console.log('is null');
                                }
                                var taches = '<li>' +
                                    '<div class="md-list-content">' +
                                    ' <div class="uk-margin-small-top">' +
                                    '<span class="uk-margin-right">' +
                                    ' <i class="material-icons">&#xE192;</i>' +
                                    ' <span class="uk-text-muted uk-text-small">' +
                                    jQuery.format.date(item.creationDate.date, 'dd/MM/yyyy HH:mm:ss')
                                    +
                                    '  </span>' +
                                    ' </span>' +
                                    '  <span class="uk-text-muted uk-text-small">';
                                var path ='';
                                if (item.mandat != null) {

                                    path = "{{ path('mandat_show' , {'id' : 'ID'}) }}";
                                    url = path.replace('ID' , item.mandat.id);
                                    taches += '<a href="'+url+'" >' +
                                        item.description +
                                        '</a>';
                                }
                                else if (item.acquisition != null) {
                                    path = "{{ path('acquisition_show' , {'id' : 'ID'}) }}";
                                    url = path.replace('ID' , item.acquisition.id);
                                    taches += '<a href="'+url+'" >' +
                                        item.description +
                                        '</a>';
                                }
                                else
                                {
                                    path = "{{ path('bien_show' , {'id' : 'ID'}) }}";
                                    url = path.replace('ID' , item.bien.id);
                                    taches += '<a href="'+url+'" >' +
                                        item.description +
                                        '</a>';
                                }

                                taches += ' </span>' +
                                    '  </div>' +
                                    '  </div>' +
                                    '  </li>';
                                $('#taches').append(taches);
                                altair_helpers.content_preloader_hide();
                            })
                        }
                        else {
                            altair_helpers.content_preloader_hide();
                            $('#taches').html('<center style="margin-top: 20px">Il n\'existe aucune action durant cette période ..</center>');
                        }

                    },
                    error: function (exception) {
                        console.log(exception.responseText);
                        UIkit.modal.alert('Vous n\'avez pas le droit de supprimer ce personnel pour des raisons de sécurité');
                    }
                });

        }
    </script>
{% endblock %}